   "source": [
    "# Reversing a list using reverse() assigned in a function\n",
    "def Reverse(lst): \n",
    "    lst.reverse() \n",
    "    return lst "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Assigning values to list\n",
    "lst = [10, 11, 12, 13, 14, 15] "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[15, 14, 13, 12, 11, 10]\n"
     ]
    }
   ],
   "source": [
    "# Printing the Reversed list\n",
    "print(Reverse(lst))"
   ]
  }
