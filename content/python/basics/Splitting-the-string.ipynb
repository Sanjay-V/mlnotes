   "source": [
    "# Assigning a string value to the variable\n",
    "input = 'New Delhi is the capital of India'"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Split the string into words by calling split() built-in function\n",
    "data = input.split(' ')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "['New', 'Delhi', 'is', 'the', 'capital', 'of', 'India']\n"
     ]
    }
   ],
   "source": [
    "# Print the splitted string\n",
    "print(data)"
   ]
  }
